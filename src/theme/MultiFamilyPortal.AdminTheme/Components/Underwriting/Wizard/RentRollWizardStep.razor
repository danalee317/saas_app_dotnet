<PortalErrorBoundary>
  <LoaderContainer Context=@Property>
    <TelerikTabStrip TabPosition="TabPosition.Top">
      <TabStripTab Title="Floor Plans">
        <UnderwritingFloorPlans Property=@Property />
      </TabStripTab>
      @if(Property.Models.Any())
      {
        <TabStripTab Title="Units">
          <UnderwritingRollUnits Property=@Property />
        </TabStripTab>
      }
    </TelerikTabStrip>
  </LoaderContainer>
</PortalErrorBoundary>
